@article{an2017direct,
  title = {Direct Observation of Chiral Currents and Magnetic Reflection in Atomic Flux Lattices},
  author = {An, Fangzhao Alex and Meier, Eric J. and Gadway, Bryce},
  year = {2017},
  journal = {Science Advances},
  volume = {3},
  number = {4},
  publisher = {{American Association for the Advancement of Science}},
  doi = {10.1126/sciadv.1602685},
  url = {https://advances.sciencemag.org/content/3/4/e1602685},
  abstract = {The prospect of studying topological matter with the precision and control of atomic physics has driven the development of many techniques for engineering artificial magnetic fields and spin-orbit interactions. Recently, the idea of introducing nontrivial topology through the use of internal (or external) atomic states as effective ``synthetic dimensions'' has garnered attraction for its versatility and possible immunity from heating. We engineer tunable gauge fields through the local control of tunneling phases in an effective two-dimensional manifold of discrete atomic momentum states. We demonstrate the ability to create homogeneous gauge fields of arbitrary value, directly imaging the site-resolved dynamics of induced chiral currents. Furthermore, we engineer the first inhomogeneous artificial gauge fields for cold atoms, observing the magnetic reflection of atoms incident upon a step-like variation of an artificial vector potential. These results open new possibilities for the study of topological phases and localization phenomena in atomic gases.},
  note = {\_eprint: https://advances.sciencemag.org/content/3/4/e1602685.full.pdf}
}

