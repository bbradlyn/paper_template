@article{tokatly2006magnetoelasticity,
  title = {Magnetoelasticity Theory of Incompressible Quantum {{Hall}} Liquids},
  author = {Tokatly, I. V.},
  year = {2006},
  month = jan,
  journal = {Phys Rev B},
  volume = {73},
  number = {20},
  pages = {205340},
