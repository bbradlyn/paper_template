@article{xu2018mnte2,
  title = {{{MnTe2}} as a Novel Promising Thermoelectric Material},
  author = {Xu, Yidong and Li, Wen and Wang, Chen and Chen, Zhiwei and Wu, Yixuan and Zhang, Xinyue and Li, Juan and Lin, Siqi and Chen, Yue and Pei, Yanzhong},
  year = {2018},
  volume = {4},
  pages = {215--220},
  issn = {2352-8478},
  doi = {10.1016/j.jmat.2018.04.001},
  url = {http://www.sciencedirect.com/science/article/pii/S2352847818300170},
  abstract = {Semiconducting manganese ditelluride (MnTe2) crystalizes in a high symmetry cubic structure with sufficient band gap and consists of nontoxic elements only, therefore is focused on in this work for its potential thermoelectric applications. This material intrinsically comes with a very low hole concentration of 1019,\"A\O cm,\`a\'i3, which can be successfully increased to 4,\"A\O{$\surd$}\'o,\"A\O 1020,\"A\O cm,\`a\'i3 through Ag-doping at Mn site. Such a broad carrier concentration enables an effective optimization on thermoelectric power factor, and the doping process effectively reduces the lattice thermal conductivity down to ,\`a\textordmasculine 0.5,\"A\O W/m-K due to the phonons scattered by additional point defects. As a result, a peak zT of ,\`a\textordmasculine 0.7 is obtained in p-type conduction. Moreover, the SPB model with acoustic scattering estimates the electronic properties well, which also enables insight into the underlying physical parameters related to the thermoelectric performance. Importantly, band structure calculation suggests a potentially higher thermoelectric performance for n-type conduction due to both higher band degeneracy and lower band effective mass. This work reveals MnTe2 is a novel promising thermoelectric material.},
  journal = {Journal of Materiomics},
  keywords = {MnTe,SPB,Thermoelectric,Transport properties},
  number = {3}
}

