@article{nayak2019resolving,
  title = {Resolving the Topological Classification of Bismuth with Topological Defects},
  author = {Nayak, Abhay Kumar and Reiner, Jonathan and Queiroz, Raquel and Fu, Huixia and Shekhar, Chandra and Yan, Binghai and Felser, Claudia and Avraham, Nurit and {Haim Beidenkopf}},
  year = {2019},
  journal = {Science Advances},
  volume = {5},
  number = {11},
  pages = {eaax6996},
  doi = {10.1126/sciadv.aax6996},
  url = {https://www.science.org/doi/abs/10.1126/sciadv.aax6996},
  abstract = {We used screw dislocations as a tool to probe spectroscopically the topological classification of electronic matter in bismuth. The growing diversity of topological classes leads to ambiguity between classes that share similar boundary phenomenology. This is the status of bulk bismuth. Recent studies have classified it as either a strong or a higher-order topological insulator, both of which host helical modes on their boundaries. We resolve the topological classification of bismuth by spectroscopically mapping the response of its boundary modes to a screw-dislocation. We find that the one-dimensional mode, on step-edges, extends over a wide energy range and does not open a gap near the screw-dislocations. This signifies that this mode binds to the screw-dislocation, as expected for a material with nonzero weak indices. We argue that the small energy gap, at the time reversal invariant momentum L, positions bismuth within the critical region of a topological phase transition between a higher-order topological insulator and a strong topological insulator with nonzero weak indices.}
}

