@article{maltoni2015basics,
  title = {Basics of {{QCD}} for the {{LHC}}: Pp \$\textbackslash rightarrow\$ {{H}} + {{X}} as a Case Study},
  shorttitle = {Basics of {{QCD}} for the {{LHC}}},
  author = {Maltoni, F},
  year = {2015},
  doi = {10.5170/CERN-2015-001.95},
  url = {https://cds.cern.ch/record/2008764},
  urldate = {2021-02-23},
  abstract = {Quantum Chromo Dynamics (QCD) provides the theoretical framework for any study of TeV scale physics at LHC. Being familiar with the basic concepts and techniques of QCD is therefore a must for any high-energy physicist. In these notes we consider Higgs production via gluon fusion as an example on how accurate and \"i{$\lnot$}exible predictions can be obtained in perturbative QCD. We start by illustrating how to calculate the total cross section at the leading order (yet one loop) in the strong coupling \^I{$\pm$}S and go through the details of the nextto-leading order calculation eventually highlighting the limitations of \"i{$\lnot$}xedorder predictions at the parton level. Finally, we brie\"i{$\lnot$}y discuss how more exclusive (and practical) predictions can be obtained through matching/merging \"i{$\lnot$}xed-order results with parton showers.},
  journal = {Proceedings of the 2013 CERN\textendash Latin-American School of High-Energy Physics},
  keywords = {Particle Physics - Phenomenology},
  language = {en},
  note = {Other Quantum Chromo Dynamics (QCD) provides the theoretical framework for any study of TeV scale physics at LHC. Being familiar with the basic concepts and techniques of QCD is therefore a must for any high-energy physicist. In these notes we consider Higgs production via gluon fusion as an example on how accurate and flexible predictions can be obtained in perturbative QCD. We start by illustrating how to calculate the total cross section at the leading order (yet one loop) in the strong coupling \^I{$\pm$}S and go through the details of the next-to-leading order calculation eventually highlighting the limitations of fixed-order predictions at the parton level. Finally, we briefly discuss how more exclusive (and practical) predictions can be obtained through matching/merging fixed-order results with parton showers.
\par
Publisher: CERN}
}

