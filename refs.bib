@article{liu2012half,
  title = {Half Quantum Spin {{Hall}} Effect on the Surface of Weak Topological Insulators},
  author = {Liu, Chao-Xing and Qi, Xiao-Liang and Zhang, Shou-Cheng},
  year = {2012},
  journal = {Physica E: Low-dimensional Systems and Nanostructures},
  volume = {44},
  number = {5},
  pages = {906--911},
  issn = {1386-9477},
  doi = {10.1016/j.physe.2011.11.005},
  url = {http://www.sciencedirect.com/science/article/pii/S1386947711004000},
  abstract = {We investigate interaction effects in three dimensional weak topological insulators (TI) with an even number of Dirac cones on the surface. We find that the surface states can be gapped by a surface charge density wave (CDW) order without breaking the time-reversal symmetry. In this sense, time-reversal symmetry alone cannot robustly protect the weak TI state in the presence of interactions. If the translational symmetry is additionally imposed in the bulk, a topologically non-trivial weak TI state can be obtained with helical edge states on the CDW domain walls. In other words, a CDW domain wall on the surface is topologically equivalent to the edge of a two-dimensional quantum spin Hall insulator. Therefore, the surface state of a weak topological insulator with translation symmetry breaking on the surface has a ``half quantum spin Hall effect'', in the same way that the surface state of a strong topological insulator with time-reversal symmetry breaking on the surface has a ``half quantum Hall effect''. The on-site and nearest neighbor interactions are investigated in the mean field level and the phase diagram for the surface states of weak topological insulators is obtained.}
}

