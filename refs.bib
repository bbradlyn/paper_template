@article{cheng2020third,
  title = {Third Order Optical Nonlinearity of Three Dimensional Massless {{Dirac}} Fermions},
  author = {Cheng, J. L. and Sipe, J. E. and Wu, S. W.},
  year = {2020},
  month = may,
  doi = {10.1021/acsphotonics.0c00836},
  url = {http://arxiv.org/abs/2005.13693},
  urldate = {2020-12-01},
  abstract = {We present analytic expressions for the electronic contributions to the linear conductivity \$\textbackslash textbackslashsigma\textasciicircum\{(1)\}\_\{3d\}(\textbackslash textbackslashomega)\$ and the third order optical conductivity \$\textbackslash textbackslashsigma\textasciicircum\{(3)\}\_\{3d\}(\textbackslash textbackslashomega\_1,\textbackslash textbackslashomega\_2,\textbackslash textbackslashomega\_3)\$ of three dimensional massless Dirac fermions, the quasi-particles relevant for the low energy excitation of topological Dirac semimetals and Weyl semimetals. Although there is no gap for massless Dirac fermions, a finite chemical potential \$\textbackslash textbackslashmu\$ can lead to an effective gap parameter, which plays an important role in the qualitative features of interband optical transitions. For gapless linear dispersion in three dimension, the imaginary part of the linear conductivity diverges as a logarithmic function of the cutoff energy, while the real part is linear with photon frequency \$\textbackslash textbackslashomega\$ as \$\textbackslash textbackslashhbar\textbackslash textbackslashomega\textbackslash textgreater2\textbackslash textbar\textbackslash textbackslashmu\textbackslash textbar\$. The third order conductivity exhibits features very similar to those of two dimensional Dirac fermions, i.e., graphene, but with the amplitude for a single Dirac cone generally two orders of magnitude smaller in three dimension than in two dimension. There are many resonances associated with the chemical potential induced gap parameters, and divergences associated with the intraband transitions. The details of the third order conductivity are discussed for third harmonic generation, the Kerr effect and two-photon carrier injection, parametric frequency conversion, and two-color coherent current injection. Although the expressions we derive are limited to the clean limit at zero temperature, the generalization to include phenomenological relaxation processes at finite temperature is straightforward and is presented.},
  archiveprefix = {arXiv},
  eprint = {2005.13693},
  eprinttype = {arxiv},
  journal = {arXiv:2005.13693 [cond-mat, physics:physics]},
  keywords = {Condensed Matter - Materials Science,Physics - Optics},
  language = {en},
  note = {arXiv: 2005.13693},
  primaryclass = {cond-mat, physics:physics}
}

