@article{zhang2019catalogue,
  title = {Catalogue of Topological Electronic Materials},
  author = {Zhang, Tiantian and Jiang, Yi and Song, Zhida and Huang, He and He, Yuqing and Fang, Zhong and Weng, Hongming and Fang, Chen},
  year = {2019},
  month = feb,
  journal = {Nature},
  volume = {566},
  number = {7745},
  pages = {475--479},
  issn = {1476-4687},
  doi = {10.1038/s41586-019-0944-6},
  url = {https://doi.org/10.1038/s41586-019-0944-6},
  abstract = {Topological electronic materials such as bismuth selenide, tantalum arsenide and sodium bismuthide show unconventional linear response in the bulk, as well as anomalous gapless states at their boundaries. They are of both fundamental and applied interest, with the potential for use in high-performance electronics and quantum computing. But their detection has so far been hindered by the difficulty of calculating topological invariant properties (or topological nodes), which requires both experience with materials and expertise with advanced theoretical tools. Here we introduce an effective, efficient and fully automated algorithm that diagnoses the nontrivial band topology in a large fraction of nonmagnetic materials. Our algorithm is based on recently developed exhaustive mappings between the symmetry representations of occupied bands and topological invariants. We sweep through a total of 39,519 materials available in a crystal database, and find that as many as 8,056 of them are topologically nontrivial. All results are available and searchable in a database with an interactive user interface.}
}

