@article{mogi2022experimental,
  title = {Experimental Signature of the Parity Anomaly in a Semi-Magnetic Topological Insulator},
  author = {Mogi, M. and Okamura, Y. and Kawamura, M. and Yoshimi, R. and Yasuda, K. and Tsukazaki, A. and Takahashi, K. S. and Morimoto, T. and Nagaosa, N. and Kawasaki, M. and Takahashi, Y. and Tokura, Y.},
  year = {2022},
  month = apr,
  journal = {Nature Physics},
  volume = {18},
  number = {4},
  pages = {390--394},
  issn = {1745-2481},
  doi = {10.1038/s41567-021-01490-y},
  url = {https://doi.org/10.1038/s41567-021-01490-y},
  abstract = {A three-dimensional (3D) topological insulator features a 2D surface state consisting of a single linearly dispersive Dirac cone1\textendash 3. Under broken time-reversal symmetry, the single Dirac cone is predicted to cause half-integer quantization of Hall conductance, which is a manifestation of the parity anomaly in quantum field theory1\textendash 9. However, despite various observations of quantization phenomena10\textendash 15, the half-integer quantization has not been observed because most experiments simultaneously measure a pair of equivalent Dirac cones16 on two opposing surfaces. Here we demonstrate the half-integer quantization of Hall conductance in a synthetic heterostructure termed a semi-magnetic topological insulator, where only one surface state is gapped by magnetic doping and the opposite one is non-magnetic and gapless. We observe half-quantized Faraday and Kerr rotations with terahertz magneto-optical spectroscopy and half-quantized Hall conductance in transport at zero magnetic field. Our results suggest a condensed-matter realization of the parity anomaly4\textendash 9 and open a way for studying the physics enabled by a single Dirac fermion.}
}

